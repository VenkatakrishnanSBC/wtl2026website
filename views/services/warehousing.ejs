<%- include('../partials/head') %>
<%- include('../partials/header') %>
<% locals.pageTitle = t('servicesWarehousing.pageTitle'); locals.pageSubtitle = t('servicesWarehousing.pageSubtitle'); locals.breadcrumb = [{label: t('servicesWarehousing.breadcrumbParent'), url: (locals.langPrefix || '') + '/services'}, {label: t('servicesWarehousing.breadcrumbCurrent')}]; %>
<%- include('../partials/page-hero') %>

<!-- Overview Section -->
<section class="section" style="padding: 80px 0;">
  <div class="container">
    <div class="about-grid" style="display: grid; grid-template-columns: 1fr 1fr; gap: 40px; align-items: center;">
      <div class="about-text">
        <span class="section-label"><%= t('servicesWarehousing.sectionLabel') %></span>
        <h2><%= t('servicesWarehousing.overviewTitle') %></h2>
        <p>
          <%= t('servicesWarehousing.overviewText1') %>
        </p>
        <p>
          <%= t('servicesWarehousing.overviewText2') %>
        </p>
        <p>
          <%= t('servicesWarehousing.overviewText3') %>
        </p>
      </div>
      <div class="about-image">
        <img src="/images/cardboard-boxes-on-shelves-in-warehouse.jpg" alt="WTL Warehouse Operations" style="width: 100%; border-radius: 12px; box-shadow: 0 10px 30px rgba(0,0,0,0.12);">
      </div>
    </div>
  </div>
</section>

<!-- Services Grid Section -->
<section class="section" style="padding: 80px 0; background: #f8f9fa;">
  <div class="container">
    <div class="section-header" style="text-align: center; margin-bottom: 50px;">
      <span class="section-label"><%= t('servicesWarehousing.warehouseServices') %></span>
      <h2><%= t('servicesWarehousing.warehouseServicesTitle') %></h2>
    </div>

    <div class="services-grid" style="display: grid; grid-template-columns: repeat(3, 1fr); gap: 30px;">
      <div class="service-card" style="background: #fff; border-radius: 12px; overflow: hidden; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
        <div class="service-card-body" style="padding: 30px;">
          <h3 style="color: #1a3a5c;"><%= t('servicesWarehousing.climateTitle') %></h3>
          <p><%= t('servicesWarehousing.climateText') %></p>
          <ul style="padding-left: 20px; color: #555; margin-top: 15px;">
            <li><%= t('servicesWarehousing.climateFeature1') %></li>
            <li><%= t('servicesWarehousing.climateFeature2') %></li>
            <li><%= t('servicesWarehousing.climateFeature3') %></li>
            <li><%= t('servicesWarehousing.climateFeature4') %></li>
          </ul>
        </div>
      </div>

      <div class="service-card" style="background: #fff; border-radius: 12px; overflow: hidden; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
        <div class="service-card-body" style="padding: 30px;">
          <h3 style="color: #1a3a5c;"><%= t('servicesWarehousing.bondedTitle') %></h3>
          <p><%= t('servicesWarehousing.bondedText') %></p>
          <ul style="padding-left: 20px; color: #555; margin-top: 15px;">
            <li><%= t('servicesWarehousing.bondedFeature1') %></li>
            <li><%= t('servicesWarehousing.bondedFeature2') %></li>
            <li><%= t('servicesWarehousing.bondedFeature3') %></li>
            <li><%= t('servicesWarehousing.bondedFeature4') %></li>
          </ul>
        </div>
      </div>

      <div class="service-card" style="background: #fff; border-radius: 12px; overflow: hidden; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
        <div class="service-card-body" style="padding: 30px;">
          <h3 style="color: #1a3a5c;"><%= t('servicesWarehousing.inventoryTitle') %></h3>
          <p><%= t('servicesWarehousing.inventoryText') %></p>
          <ul style="padding-left: 20px; color: #555; margin-top: 15px;">
            <li><%= t('servicesWarehousing.inventoryFeature1') %></li>
            <li><%= t('servicesWarehousing.inventoryFeature2') %></li>
            <li><%= t('servicesWarehousing.inventoryFeature3') %></li>
            <li><%= t('servicesWarehousing.inventoryFeature4') %></li>
          </ul>
        </div>
      </div>
    </div>

    <!-- Second Row -->
    <div class="services-grid" style="display: grid; grid-template-columns: repeat(3, 1fr); gap: 30px; margin-top: 30px;">
      <div class="service-card" style="background: #fff; border-radius: 12px; overflow: hidden; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
        <div class="service-card-body" style="padding: 30px;">
          <h3 style="color: #1a3a5c;"><%= t('servicesWarehousing.pickPackTitle') %></h3>
          <p><%= t('servicesWarehousing.pickPackText') %></p>
          <ul style="padding-left: 20px; color: #555; margin-top: 15px;">
            <li><%= t('servicesWarehousing.pickPackFeature1') %></li>
            <li><%= t('servicesWarehousing.pickPackFeature2') %></li>
            <li><%= t('servicesWarehousing.pickPackFeature3') %></li>
            <li><%= t('servicesWarehousing.pickPackFeature4') %></li>
          </ul>
        </div>
      </div>

      <div class="service-card" style="background: #fff; border-radius: 12px; overflow: hidden; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
        <div class="service-card-body" style="padding: 30px;">
          <h3 style="color: #1a3a5c;"><%= t('servicesWarehousing.crossDockTitle') %></h3>
          <p><%= t('servicesWarehousing.crossDockText') %></p>
          <ul style="padding-left: 20px; color: #555; margin-top: 15px;">
            <li><%= t('servicesWarehousing.crossDockFeature1') %></li>
            <li><%= t('servicesWarehousing.crossDockFeature2') %></li>
            <li><%= t('servicesWarehousing.crossDockFeature3') %></li>
            <li><%= t('servicesWarehousing.crossDockFeature4') %></li>
          </ul>
        </div>
      </div>

      <div class="service-card" style="background: #fff; border-radius: 12px; overflow: hidden; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
        <div class="service-card-body" style="padding: 30px;">
          <h3 style="color: #1a3a5c;"><%= t('servicesWarehousing.valueAddedTitle') %></h3>
          <p><%= t('servicesWarehousing.valueAddedText') %></p>
          <ul style="padding-left: 20px; color: #555; margin-top: 15px;">
            <li><%= t('servicesWarehousing.valueAddedFeature1') %></li>
            <li><%= t('servicesWarehousing.valueAddedFeature2') %></li>
            <li><%= t('servicesWarehousing.valueAddedFeature3') %></li>
            <li><%= t('servicesWarehousing.valueAddedFeature4') %></li>
          </ul>
        </div>
      </div>
    </div>
  </div>
</section>

<!-- Why WTL Warehousing -->
<section class="section" style="padding: 80px 0;">
  <div class="container">
    <div class="section-header" style="text-align: center; margin-bottom: 50px;">
      <span class="section-label"><%= t('servicesWarehousing.advantage') %></span>
      <h2><%= t('servicesWarehousing.whyTitle') %></h2>
    </div>

    <div class="why-grid" style="display: grid; grid-template-columns: repeat(4, 1fr); gap: 30px;">
      <div class="why-card" style="text-align: center; padding: 30px; background: #fff; border-radius: 12px; box-shadow: 0 5px 20px rgba(0,0,0,0.06);">
        <div class="why-icon" style="font-size: 2.5rem; margin-bottom: 15px;">&#128205;</div>
        <h4><%= t('servicesWarehousing.whyLocationTitle') %></h4>
        <p style="color: #666;"><%= t('servicesWarehousing.whyLocationText') %></p>
      </div>
      <div class="why-card" style="text-align: center; padding: 30px; background: #fff; border-radius: 12px; box-shadow: 0 5px 20px rgba(0,0,0,0.06);">
        <div class="why-icon" style="font-size: 2.5rem; margin-bottom: 15px;">&#128274;</div>
        <h4><%= t('servicesWarehousing.whySecurityTitle') %></h4>
        <p style="color: #666;"><%= t('servicesWarehousing.whySecurityText') %></p>
      </div>
      <div class="why-card" style="text-align: center; padding: 30px; background: #fff; border-radius: 12px; box-shadow: 0 5px 20px rgba(0,0,0,0.06);">
        <div class="why-icon" style="font-size: 2.5rem; margin-bottom: 15px;">&#128200;</div>
        <h4><%= t('servicesWarehousing.whyScalableTitle') %></h4>
        <p style="color: #666;"><%= t('servicesWarehousing.whyScalableText') %></p>
      </div>
      <div class="why-card" style="text-align: center; padding: 30px; background: #fff; border-radius: 12px; box-shadow: 0 5px 20px rgba(0,0,0,0.06);">
        <div class="why-icon" style="font-size: 2.5rem; margin-bottom: 15px;">&#128279;</div>
        <h4><%= t('servicesWarehousing.whyIntegratedTitle') %></h4>
        <p style="color: #666;"><%= t('servicesWarehousing.whyIntegratedText') %></p>
      </div>
    </div>
  </div>
</section>

<!-- CTA Banner -->
<section class="cta-banner" style="padding: 80px 0; background: linear-gradient(135deg, #1a3a5c, #2a5a8c); text-align: center; color: #fff;">
  <div class="container">
    <h2 style="font-size: 2.2rem; margin-bottom: 15px;"><%= t('servicesWarehousing.ctaTitle') %></h2>
    <p style="font-size: 1.15rem; margin-bottom: 30px; opacity: 0.9; max-width: 600px; margin-left: auto; margin-right: auto;">
      <%= t('servicesWarehousing.ctaText') %>
    </p>
    <a href="<%= locals.langPrefix || '' %>/quote" class="btn-quote" style="display: inline-block; padding: 15px 40px; background: #f0a500; color: #1a3a5c; font-weight: 700; text-decoration: none; border-radius: 8px; font-size: 1.1rem;"><%= t('servicesWarehousing.ctaButton') %></a>
  </div>
</section>

<%- include('../partials/footer') %>